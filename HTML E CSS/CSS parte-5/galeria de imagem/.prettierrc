{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "htmlWhitespaceSensitivity": "ignore",
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html",
        "tabWidth": 4
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css",
        "tabWidth": 4
      }
    }
  ]
}
